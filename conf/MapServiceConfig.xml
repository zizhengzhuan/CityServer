<?xml version="1.0" encoding="utf-8"?>
<ConfigGroup brief="地图服务配置文件" name="MAPSERVICE-CONFIG">
	<ConfigItem brief="定义地图服务的名称" name="Name">
		<value>地图服务</value>
		<description>map Service。</description>
		<defvalue />
		<editable>1</editable>
		<dispstyle>TextBox</dispstyle>
		<datatype>
			<typename>String</typename>
			<length>128</length>
			<minvalue>0</minvalue>
			<maxvalue>0</maxvalue>
		</datatype>
		<selectdata />
	</ConfigItem>
	<ConfigItem brief="服务重启控制" name="START_CTR">
		<value>0</value>
		<description>0 不自动重启,1-4 服务实例空闲2个小时重启，2-5:30 每天5:30自动重启</description>
		<defvalue>3</defvalue>
		<editable>1</editable>
		<dispstyle>TextBox</dispstyle>
		<datatype>
			<typename>int</typename>
			<length>10</length>
			<minvalue>3</minvalue>
			<maxvalue>10</maxvalue>
		</datatype>
		<selectdata />
	</ConfigItem>
	<ConfigItem brief="打开地图文档实例数" name="DOC_OPEN_COUNT">
		<value>3</value>
		<description></description>
		<defvalue>3</defvalue>
		<editable>1</editable>
		<dispstyle>TextBox</dispstyle>
		<datatype>
			<typename>int</typename>
			<length>10</length>
			<minvalue>3</minvalue>
			<maxvalue>10</maxvalue>
		</datatype>
		<selectdata />
	</ConfigItem>

	<ConfigItem brief="地图服务结果路径" name="MAP_RESULT_PATH">
		<value>map</value>
		<description>地图服务结果路径,如不设置，自动设置为ServiceEngine/data</description>
		<defvalue>3</defvalue>
		<editable>1</editable>
		<dispstyle>TextBox</dispstyle>
		<datatype>
			<typename>String</typename>
			<length>10</length>
			<minvalue>3</minvalue>
			<maxvalue>10</maxvalue>
		</datatype>
		<selectdata />
	</ConfigItem>
	<ConfigItem brief="地图服务结果URL" name="MAP_RESULT_URL">
		<value>map</value>
		<description>地图服务结果URL,MAP_RESULT_PATH对应的虚拟目录</description>
		<defvalue>3</defvalue>
		<editable>1</editable>
		<dispstyle>TextBox</dispstyle>	
		<datatype>
			<typename>String</typename>
			<length>10</length>
			<minvalue>3</minvalue>
			<maxvalue>10</maxvalue>
		</datatype>
		<selectdata />	
	</ConfigItem>
	<ConfigItem brief="是否需要Token" name="Need_Token">
		<value>false</value>
		<description></description>
		<defvalue>3</defvalue>
		<editable>1</editable>
		<dispstyle>TextBox</dispstyle>
		<datatype>
			<typename>boolean</typename>
			<length>10</length>
			<minvalue>3</minvalue>
			<maxvalue>10</maxvalue>
		</datatype>
		<selectdata />
	</ConfigItem>	
</ConfigGroup>
